{
  "name": "domain-example",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "app-grouping-folder": "ng generate @srleecode/domain:appGroupingFolder --name=test-app --no-interactive",
    "domain-grouping-folder": "ng generate @srleecode/domain:domainGroupingFolder --groupingFolder=libs/ng-test-app --name=test-domain --no-interactive",
    "application-layer": "ng generate @srleecode/domain:ngApplicationLayer --groupingFolder=libs/ng-test-app/test-domain --no-interactive",
    "data-access-layer": "ng generate @srleecode/domain:ngDataAccessLayer --groupingFolder=libs/ng-test-app/test-domain --no-interactive",
    "domain-layer": "ng generate @srleecode/domain:ngDomainLayer --groupingFolder=libs/ng-test-app/test-domain --no-interactive",
    "util-layer": "ng generate @srleecode/domain:ngUtilLayer --groupingFolder=libs/ng-test-app/test-domain --no-interactive",
    "shell": "ng generate @srleecode/domain:ngComponent --groupingFolder=libs/ng-test-app/test-domain --type=shell --name=test-example --no-interactive",
    "feature": "ng generate @srleecode/domain:ngComponent --groupingFolder=libs/ng-test-app/test-domain --type=feature --name=test-example --no-interactive",
    "ui": "ng generate @srleecode/domain:ngComponent --groupingFolder=libs/ng-test-app/test-domain --type=ui --name=test-example --no-interactive",
    "story-ui": "ng generate @srleecode/domain:ngComponent --groupingFolder=libs/ng-test-app/test-domain --type=ui --name=story-test-example --mountType=story --unitTestType=noTestBed --no-interactive",
    "directive": "ng generate @srleecode/domain:ngDirective --groupingFolder=libs/ng-test-app/test-domain --name=test-example --no-interactive",
    "story-directive": "ng generate @srleecode/domain:ngDirective --groupingFolder=libs/ng-test-app/test-domain --name=second-test --mountType=story --unitTestType=testBed",
    "domain-tests": "ng generate @srleecode/domain:domainTest --groupingFolder=libs/ng-test-app/test-domain --no-interactive",
    "create-example": "npm run app-grouping-folder && npm run domain-grouping-folder && npm run application-layer && npm run data-access-layer && npm run domain-layer && npm run util-layer && npm run shell && npm run feature && npm run ui && npm run story-ui && npm run directive && npm run story-directive && npm run domain-tests",
    "affected:ct": "nx affected --target=ct",
    "ct-test": "nx run ng-test-app-test-domain-ui-story-test-example:ct",
    "migrate": "nx -- migrate --run-migrations",
    "compile": "tsc --p tools/domain-dep-graph/ts-config.lib.json",
    "dep-graph": "npm run compile && npx nx dep-graph --file=graph.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.2.0",
    "@angular/common": "~14.2.0",
    "@angular/compiler": "~14.2.0",
    "@angular/core": "~14.2.0",
    "@angular/forms": "~14.2.0",
    "@angular/platform-browser": "~14.2.0",
    "@angular/platform-browser-dynamic": "~14.2.0",
    "@angular/router": "~14.2.0",
    "@nrwl/angular": "15.0.0",
    "@srleecode/domain": "^15.0.1",
    "@storybook/core-server": "6.5.12",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.0",
    "@angular-eslint/eslint-plugin": "~14.0.4",
    "@angular-eslint/eslint-plugin-template": "~14.0.4",
    "@angular-eslint/template-parser": "~14.0.4",
    "@angular/cdk": "14.2.5",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "~14.2.0",
    "@angular/language-service": "~14.2.0",
    "@nrwl/angular": "15.0.0",
    "@nrwl/cli": "15.0.0",
    "@nrwl/cypress": "15.0.0",
    "@nrwl/eslint-plugin-nx": "15.0.0",
    "@nrwl/jest": "15.0.0",
    "@nrwl/linter": "15.0.0",
    "@nrwl/storybook": "15.0.0",
    "@nrwl/tao": "15.0.0",
    "@nrwl/workspace": "15.0.0",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/angular": "6.5.12",
    "@storybook/builder-webpack5": "6.5.12",
    "@storybook/manager-webpack5": "6.5.12",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "cypress": "^10.7.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "~12.2.2",
    "prettier": "2.7.1",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
